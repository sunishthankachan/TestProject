                            <c:if test="${fileVO.fileExtension == '.zip'}">
                                <img src="images/icon_zip.gif" />
                            </c:if>
                            <c:if test="${fileVO.fileExtension == '.txt'}">
                                <img src="images/icon_txtfile.bmp" />
                            </c:if>
                            <c:if test="${fileVO.fileExtension == '.docx'}">
                                <img src="images/icon_docx.jpeg" />
                            </c:if>
                            <c:if test="${fileVO.fileExtension == '.xlsx'}">
                                <img src="images/icon_xlsx.jpeg" />
                            </c:if>
                            <c:if test="${fileVO.fileExtension == '.docm'}">
                                <img src="images/icon_docm.jpeg" />
                            </c:if>
                            <c:if test="${fileVO.fileExtension == '.xlsm'}">
                                <img src="images/icon_xlsm.jpeg" />
                            </c:if>
                            <c:if test="${fileVO.fileExtension == '.html'}">
                                <!-- html has no icon -->
                            </c:if>
                            <c:out value="${fileVO.fileType}" /> (<c:out value="${fileVO.fileExtension}" />)
                        </td>

                        <!-- MR1402 Tech refresh - moving to Office 2010 - Ends-->
                        <td width="100"><c:out value="${fileVO.description}" /> </td>
                        <td width="125"><c:out value="${fileVO.dateAndTime}" /> </td>

                        <td width="30"><c:out value="${fileVO.uploadedBy}" /> </td>
                        <td><a href="javascript:viewdoc('<c:out value="${fileVO.oid}"/>')" id="viewAttachment1" title="view this document">View</a></td>
                        <!-- NP16086 - Allow incorrect attachments to be removed from completed applications for a newly created AD role:Starts -->
                        <td>
                            <c:choose>
                                <c:when test="${(!listLendingApplicationDocForm.disabled
                                    or listLendingApplicationDocForm.documentAtachmentVO.maintainAttachmentsRole)
                                    and fileVO.categoryCode ne 'ECR'}">
                                    <a href="javascript:removedoc('${fileVO.oid}')"
                                       title="remove attachment from Lending Application">Remove</a>
                                </c:when>
                                <c:when test="${listLendingApplicationDocForm.documentAtachmentVO.maintainAttachmentsRole
                                    and fileVO.categoryCode eq 'ECR'}">
                                    <a href="javascript:removedoc('${fileVO.oid}')"
                                       title="remove attachment from Lending Application">Remove</a>
                                </c:when>
                                <c:otherwise>
                                    <!-- empty cell -->
                                </c:otherwise>
                            </c:choose>
                        </td>

                        <!-- NP16086 - Allow incorrect attachments to be removed from completed applications for a newly created AD role:Ends -->
                    </tr>
                </c:forEach>

            </table>
        </c:when>
        <c:otherwise>
            <table>
                <tr>
                    <td>No documents are currently attached to the lending application</td>
                </tr>
            </table>
        </c:otherwise>
    </c:choose>
    <table>
        <tr>    &nbsp;&nbsp;
            <td>
                <a href="javascript:viewattach();" id="uploadattach1" title="upload a file">Upload a file</a>
            </td>
        </tr>

    </table>


