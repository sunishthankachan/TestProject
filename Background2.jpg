// onLoad.js

// doOnLoad
function doOnLoad() {
	injectAllSelectBoxes();
	injectAllCancel();
	injectAllInputs();
	injectAllTextBox();
	injectAllTextArea();

	//IE8 Upgrade - To prevent IE8 submitting the page twice. - starts
	if(!buttonsInjected) {
	injectAllButtons();
	}
	//IE8 Upgrade - To prevent IE8 submitting the page twice. - ends
}

// injectAllSelectBoxes
selectBoxReturnValue=true;
function injectAllSelectBoxes() {
   var elements = document.getElementsByTagName("select");
   for (var i in elements) {
       var el = elements[i];

       if (el.id) {
           el.onkeydown = function(ev) {
               return updateSelection(this, window.event);
           }

           el.onblur = function() {
           	   // Due to an IE bug, we set the selectedIndex *again* as we leave
           	   // because, in some cases, the field will have reset back to its
           	   // original selectedIndex.
           	   if (lastSelectedIndex!=-1)
           	   {
	           	   this.selectedIndex = lastSelectedIndex;
	           	   lastSelectedIndex=-1;
	           }
               resetBuffer(); 
               checkValue();
           }
           
           el.onfocus = function() { 
           	   storeValue();
               resetBuffer();
               // Due to an IE bug, we pull out the value that was set prior to
               // focus being set -- See onfocusin
               //this.selectedIndex = this.tmpIndex;
           }
           
           // Due to an IE bug, when focus first arrives on this control, we
           // set the selected index so that the onfocus event can restore it
           // (IE resets the selection of the select box to the first entry if
           // this code isn't here)
           el.onfocusin = function() {
           	   this.tmpIndex = this.selectedIndex;
           }
       }
   }
}

// Set the cancelled variable (defined in ceeCommon.js) to true
function setCancelled() 
{
	cancelled=true;
}

// Attach setCancelled method to onclick event of all inputs with a given name 
// and id 
function injectAllCancel() {
   var elements = document.getElementsByTagName("input");
   for (var i in elements) {
      var el = elements[i];
      
      if (el.name=="org.apache.struts.taglib.html.CANCEL") { 
      	if (el.id=="cancel") {
			el.attachEvent('onclick', setCancelled);
		}
      }
   }
}

// Attach storeValue and checkValue to the onfocus and onblur events of all inputs
function injectAllInputs() {

	var elements = document.getElementsByTagName("input");
   
	for (x=0; x<elements.length; x++) {
    	var el = elements[x];
  	  	el.attachEvent('onfocus', storeValue);
		el.attachEvent('onblur', checkValue);
	}
}

