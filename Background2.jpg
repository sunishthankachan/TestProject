// Variable used to store the value of a field onFocus
var currentvalue;

// Stores the value (or checked value) of an input onFocus
function storeValue() {
	// PLNV NZLO2  -Start
	//Below code added for to avoid script error in JPSs 
	//Script error will occur if there is no ObjectIdentifierId and it is not available in all JSPs
	//other than viewEditExistingPersonalLoan.jsp and personalLoan.jsp. 
	if(eval (document.getElementById('ObjectIdentifierId')) != null) {
		document.getElementById('ObjectIdentifierId').value=event.srcElement.id;	
	}
	// PLNV NZLO2  -End
	if (event.srcElement.type=="radio" || event.srcElement.type=='checkbox' ) {
		currentvalue = event.srcElement.checked;
	} else {
		currentvalue = event.srcElement.value;
	}
	
}

// Checks the value (or checked value) of an input onBlur and sees if it has 
// changed since the onFocus event. If it has, set the modified flag (set in
// ceeCommon.js) to true
function checkValue() {

	if (event.srcElement.type=="radio" || event.srcElement.type=='checkbox' ) {
		newvalue = event.srcElement.checked;
	} else {
		newvalue = event.srcElement.value;
	}
	
	if (newvalue!=currentvalue)
	{
		modified=true;
	}
}

// Attach checkForInvalidCharacters to the onkeypress event of all text box inputs
function injectAllTextBox() {
	var elements = document.getElementsByTagName("input");
   
	for (x=0; x<elements.length; x++) {
    	var el = elements[x];

    	if (el.type=="text")
    	{
	  	  	el.attachEvent('onkeypress', checkForInvalidCharacters);
	  	}
	}
}

// Checks to ensure that an invalid character cannot be entered. Currently this is
// only the ^ character.  
var invalidCharWarningGiven = false;
function checkForInvalidCharacters() {
	
	var keyCodePressed = event.keyCode;
	
	// Disallow ^ (keycode=94)
	if (keyCodePressed == 94)
	{
		if ( !invalidCharWarningGiven )
		{
			alert("Invalid character");
			invalidCharWarningGiven = true;
		}
		return false;
	}
	
	return true;
}




